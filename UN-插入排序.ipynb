   ],
   "source": [
    "array = [7, 6, 5, 3, 4, 1, 2]\n",
    "\n",
    "for i in range(1, len(array)):\n",
    "    for j in range(i-1, -1, -1):\n",
    "        if array[j+1] < array[j]:\n",
    "            array[j+1], array[j] = array[j], array[j+1]\n",
    "print('result:', array)"
   ]
  }
